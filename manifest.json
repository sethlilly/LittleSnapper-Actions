{
  "name": "LittleSnapper Actions",
  "version": "0.1.1",
  "description": "Provides simplified LittleSnapper bookmarklets as an extension.",
  "permissions": ["tabs"],
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "LittleSnapper Actions",
    "default_popup": "popup.html"
  },
  "icons": {
  	"16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}